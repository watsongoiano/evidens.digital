{
  "version": 2,
  "name": "evidens-digital-auth",
  "builds": [
    {
      "src": "app_with_auth.py",
      "use": "@vercel/python"
    },
    {
      "src": "login.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/app_with_auth.py"
    },
    {
      "src": "/logout",
      "dest": "/app_with_auth.py"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app_with_auth.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "SECRET_KEY": "@secret_key",
    "DATABASE_URL": "@database_url",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "APPLE_CLIENT_ID": "@apple_client_id",
    "APPLE_CLIENT_SECRET": "@apple_client_secret"
  },
  "functions": {
    "app_with_auth.py": {
      "runtime": "python3.9"
    }
  }
}
