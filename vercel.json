{
  "version": 2,
  "name": "evidens-digital",
  "builds": [
    {
      "src": "build.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "outputDirectory": "dist",
  "framework": null,
  "buildCommand": "python build.py"
}