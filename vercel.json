{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "intelligent-tools.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/checkup-intelligent.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gerar-solicitacao-exames.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gerar-receita-vacinas.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/checkup-intelligent",
      "dest": "/api/checkup-intelligent.py"
    },
    {
      "src": "/api/gerar-solicitacao-exames",
      "dest": "/api/gerar-solicitacao-exames.py"
    },
    {
      "src": "/api/gerar-receita-vacinas",
      "dest": "/api/gerar-receita-vacinas.py"
    },
    {
      "src": "/prevent_calculator.js",
      "dest": "/prevent_calculator.js"
    },
    {
      "src": "/evidenscheck.svg",
      "dest": "/evidenscheck.svg"
    },
    {
      "src": "/evidenscheck.png",
      "dest": "/evidenscheck.png"
    },
    {
      "src": "/intelligent-tools",
      "dest": "/intelligent-tools.html"
    },
    {
      "src": "/(?!.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|pdf|txt|xml|json)).*",
      "dest": "/index.html"
    }
  ]
}
