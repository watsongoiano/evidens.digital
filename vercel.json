{
  "rewrites": [
    {
      "source": "/api/checkup",
      "destination": "/api/checkup.py"
    },
    {
      "source": "/api/analytics/(.*)",
      "destination": "/api/analytics.py"
    },
    {
      "source": "/checkup-intelligent",
      "destination": "/api/checkup-intelligent.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}