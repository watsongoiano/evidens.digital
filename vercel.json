{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/checkup",
      "dest": "/api/checkup.py"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/api/analytics.py"
    },
    {
      "src": "/checkup-intelligent",
      "dest": "/api/checkup-intelligent.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}