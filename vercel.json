{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/dashboard", 
      "dest": "/dashboard.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/intelligent-tools",
      "dest": "/intelligent-tools.html"
    },
    {
      "src": "/",
      "dest": "/src/static/index.html"
    }
  ]
}
