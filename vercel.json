{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.ico",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/checkup_intelligent_v3.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gerar-solicitacao-exames.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gerar-receita-vacinas.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/checkup_intelligent_v3",
      "dest": "/api/checkup_intelligent_v3.py"
    },
    {
      "src": "/api/checkup-intelligent",
      "dest": "/api/checkup_intelligent_v3.py"
    },
    {
      "src": "/api/gerar-solicitacao-exames",
      "dest": "/api/gerar-solicitacao-exames.py"
    },
    {
      "src": "/api/gerar-receita-vacinas",
      "dest": "/api/gerar-receita-vacinas.py"
    },
    {
      "src": "/intelligent-tools",
      "dest": "/intelligent-tools.html"
    },
    {
      "src": "/analytics",
      "dest": "/analytics.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
