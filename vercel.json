{
  "routes": [
    { "src": "/checkup-intelligent", "dest": "/api/checkup-intelligent" },
    { "src": "/gerar-solicitacao-exames", "dest": "/api/gerar-solicitacao-exames" },
    { "src": "/gerar-receita-vacinas", "dest": "/api/gerar-receita-vacinas" },
    { "src": "/api/analytics/(.*)", "dest": "/api/analytics.py" },
    { "src": "/analytics", "dest": "/analytics.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(checkup|checkup-intelligent)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  }
}
